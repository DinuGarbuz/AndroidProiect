<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context=".MedicFragment">



    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="260dp">

        <ImageView
            android:id="@+id/imageView5"
            android:layout_width="wrap_content"
            android:layout_height="300dp"
            android:layout_marginStart="8dp"
            android:layout_marginLeft="8dp"
            android:layout_marginEnd="8dp"
            android:layout_marginRight="8dp"
            android:layout_marginBottom="16dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:srcCompat="@drawable/background2" />

        <ImageView
            android:id="@+id/imageView6"
            android:layout_width="162dp"
            android:layout_height="162dp"
            android:layout_marginBottom="36dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.497"
            app:layout_constraintStart_toStartOf="parent"
            app:srcCompat="@drawable/medic" />

        <TextView
            android:id="@+id/textView6"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginBottom="12dp"
            android:text="Garbuz Dinu"
            android:textSize="24dp"
            android:textStyle="bold"
            app:layout_constraintBottom_toTopOf="@+id/imageView6"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent" />


    </androidx.constraintlayout.widget.ConstraintLayout>

    <Button
        android:id="@+id/button_vizionare_fise"
        android:layout_width="170dp"
        android:layout_height="50dp"
        android:layout_marginTop="280dp"
        android:background="@drawable/buttcolor2"
        android:layout_gravity="center_horizontal"
        android:text="Vizualizare fise medicale"
        android:layout_weight="1"
        />


    <Button
        android:id="@+id/button_editare_contact"
        android:layout_width="170dp"
        android:layout_height="50dp"
        android:layout_marginTop="350dp"
        android:layout_weight="1"
        android:layout_gravity="center_horizontal"
        android:background="@drawable/buttcolor2"
        android:text="editare contact" />

    <Button
        android:id="@+id/button3"
        android:layout_gravity="center_horizontal"
        android:layout_weight="1"
        android:layout_width="170dp"
        android:layout_height="50dp"
        android:layout_marginTop="420dp"
        android:background="@drawable/buttcolor2"
        android:text="Info" />

    <Button
        android:id="@+id/button4"
        android:layout_width="170dp"
        android:layout_height="50dp"
        android:layout_marginTop="490dp"
        android:layout_weight="1"
        android:layout_gravity="center_horizontal"
        android:background="@drawable/buttcolor2"

        android:text="Log Out" />
    

</FrameLayout>